 
 List<GAL> listaGal = new List<GAL>();
   foreach (Utente utente in list_utente)
   {
   if(Utente.sesso == "M"){
    GAL singoloGal = new GAL();
     singoloGal.Nome = Utente.nome;
     singoloGal.Cognome = Utente.cognome;
    listaGal.Add(singoloGal);
   }
   
    return listaGal
    
   
    }
